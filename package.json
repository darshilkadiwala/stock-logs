{
  "name": "stock-logs",
  "private": true,
  "type": "module",
  "version": "0.1.0",
  "main": "dist/build/main.cjs",
  "description": "Stock Logs Application",
  "author": {
    "name": "Darshil"
  },
  "scripts": {
    "build": "react-router build",
    "clean": "rimraf dist build out .vite node_modules/.vite",
    "dev": "react-router dev",
    "dev:electron": "concurrently -k \"pnpm dev\" \"wait-on tcp:5173 && electron-forge start\"",
    "format": "prettier --write --list-different --cache --ignore-path .prettierignore .",
    "make": "pnpm build && electron-forge make",
    "package": "electron-forge package",
    "postinstall": "pnpm format",
    "start:web": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc"
  },
